    "convict-format-with-validator": "6.2.0",
    "express": "4.18.2",
    "glob": "10.3.10",
    "http-status-codes": "2.3.0",
    "jose": "5.2.2",
    "mime-types": "2.1.35",
    "mongoose": "8.1.1",
    "multer": "1.4.5-lts.1",
    "nanoid": "5.0.6",
