    "@typescript-eslint/eslint-plugin": "6.7.0",
    "@typescript-eslint/parser": "6.7.0",
    "dotenv": "16.4.1",
    "eslint": "8.49.0",
    "eslint-config-htmlacademy": "9.1.1",
    "express-async-handler": "1.2.0",
    "inversify": "6.0.2",
    "json-server": "1.0.0-alpha.21",
    "nodemon": "3.0.3",
    "pino-pretty": "10.3.1",
    "reflect-metadata": "0.2.1",
    "rimraf": "5.0.1",
    "ts-node": "10.9.1",
